<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.rt.aa.Card" table="CARD">
      <meta attribute="class-description">
         This class contains the CARD detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
     <property name="cardno" column="cardno" type="int"/>
      <property name="balance" column="balance" type="int"/>
      <property name="cvv" column="cvv" type="int"/>
      <property name="cardholder" column="cardholder" type="string"/>
              
      
 
	   
   </class>
   
    
</hibernate-mapping>